@model IEnumerable<AutoStop.Models.BusyPlaces>

@{
    ViewData["Title"] = "ReservationList";
}

<h1>Мои места</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Traverl)
            </th>
            <th>
                Точка отправления
            </th>
            <th>
                Точка прибытия
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BusyPlace)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Traverl.Number)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Traverl.PointArrive.PointName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Traverl.PointSend.PointName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BusyPlace)
            </td>
        </tr>
}
    </tbody>
</table>
